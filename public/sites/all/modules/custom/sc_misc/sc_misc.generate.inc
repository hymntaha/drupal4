function sc_misc_create_store_credit($code, $amount) {
  $form_state['values'] = array(
    'name'                 => '$' . $amount . ' store credit',
    'code'                 => $code,
    'status'               => TRUE,
    'discount'             => $amount,
    'store_credit'         => TRUE,
    'apply_to'             => 'subtotal',
    'use_validity'         => TRUE,
    'valid_from'           => array(
      'month' => 11,
      'day'   => 28,
      'year'  => 2016,
    ),
    'valid_until'          => array(
      'month' => 12,
      'day'   => 31,
      'year'  => 2017,
    ),
    'line_items'           => array(
      'shipping' => 'shipping',
    ),
    'bulk_generate'        => FALSE,
    'bulk_number'          => NULL,
    'bulk_length'          => NULL,
    'apply_count'          => '',
    'max_uses'             => '',
    'max_uses_per_user'    => '',
    'minimum_order'        => '',
    'minimum_qty'          => '',
    'minimum_qty_restrict' => FALSE,
    'product_types'        => array(),
    'negate_products'      => 0,
    'negate_terms'         => 0,
    'negate_users'         => 0,
    'negate_roles'         => 0,
    'negate_combinations'  => 0,
    'combinations'         => array(),
    'line_item_format'     => 'Store Credit',
    'apply_message'        => '',
    'op'                   => t('Save coupon'),
  );

  module_load_include('inc', 'uc_coupon', 'uc_coupon.admin');

  drupal_form_submit('uc_coupon_add_form', $form_state);
}
